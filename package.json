{
  "name": "git-involved.elm",
  "description": "Github issue filtering for OSS written in Elm",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Justin Kovalchuk",
  "homepage": "https://github.com/lamogura/git-involved.elm",
  "repository": {
    "type": "git",
    "url": "https://github.com/lamogura/git-involved.elm"
  },
  "engines": {
    "node": "latest",
    "yarn": "latest"
  },
  "bugs": {
    "url": "https://github.com/lamogura/git-involved.elm/issues"
  },
  "scripts": {
    "test": "./node_modules/.bin/elm-test tests/RunTestsInTerminal.elm",
    "start": "node server.js",
    "server": "nodemon server.js",
    "prebuild": "rimraf public/*",
    "build": "elm make app/App.elm --output=frontend/js/elm.js && yarn run build:assets && yarn run build:css && grunt && yarn build:js && yarn run build:html",
    "build:assets": "cp -r frontend/assets public/assets && mkdir public/styles",
    "build:css": "cleancss -o public/styles/index.css frontend/styles/index.css",
    "build:js": "uglifyjs frontend/js/elm.js -c -o public/js/elm.js && uglifyjs public/js/analyticsBase.js -c -o public/js/analyticsBase.js",
    "build:mark": "hashmark -c public -r -l 6 -m public/assets.json 'assets/**/*.*' '{dir}/{name}-{hash}{ext}' && hashmark -s -c public -r -l 6 -m public/assets.json '**/*.{css,js}' '{dir}/{name}-{hash}{ext}'",
    "build:html": "html-minifier --config-file htmlMinifier.conf --output public/index.html frontend/index.html",
    "dev": "elm-live --output=frontend/elm.js app/App.elm --pushstate --open --debug --dir=frontend"
  },
  "dependencies": {
    "apicache": "^0.8.6",
    "autotrack": "^2.4.1",
    "babel-core": "^6.25.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "compression": "^1.6.2",
    "cors": "^2.8.3",
    "dotenv-safe": "^4.0.4",
    "express": "^4.15.3",
    "namespaced-console-logger": "^1.0.1",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "serve-favicon": "^2.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-inline-environment-variables": "^0.1.1",
    "babelify": "^7.3.0",
    "clean-css-cli": "^4.1.4",
    "elm": "^0.18.0",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-imagemin": "^1.0.1",
    "hashmark": "3.1.0",
    "html-minifier": "^3.5.2",
    "nodemon": "^1.11.0",
    "rimraf": "^2.6.1",
    "uglify-js": "^3.0.15"
  }
}
